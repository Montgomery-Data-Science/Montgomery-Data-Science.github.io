<div class=" highlight hl-ipython3"><pre><span></span><span class="n">Growth_DF</span><span class="o">=</span><span class="n">train</span><span class="o">.</span><span class="n">groupby</span><span class="p">([</span><span class="s1">&#39;date_block_num&#39;</span><span class="p">,</span><span class="s1">&#39;shop_id&#39;</span><span class="p">])</span><span class="o">.</span><span class="n">sum</span><span class="p">()</span>
<span class="n">Growth_DF</span><span class="o">=</span><span class="n">Growth_DF</span><span class="o">.</span><span class="n">reset_index</span><span class="p">()</span>
<span class="n">shops</span><span class="o">=</span><span class="p">[</span><span class="mi">25</span><span class="p">,</span><span class="mi">31</span><span class="p">,</span><span class="mi">54</span><span class="p">]</span>
<span class="n">Growth_DF</span><span class="o">=</span><span class="n">Growth_DF</span><span class="o">.</span><span class="n">loc</span><span class="p">[</span><span class="n">Growth_DF</span><span class="p">[</span><span class="s1">&#39;shop_id&#39;</span><span class="p">]</span><span class="o">.</span><span class="n">isin</span><span class="p">(</span><span class="n">shops</span><span class="p">)]</span>

<span class="n">shop_25_DF</span><span class="o">=</span><span class="n">Growth_DF</span><span class="p">[</span><span class="n">Growth_DF</span><span class="p">[</span><span class="s1">&#39;shop_id&#39;</span><span class="p">]</span><span class="o">==</span><span class="mi">25</span><span class="p">]</span>
<span class="n">shop_31_DF</span><span class="o">=</span><span class="n">Growth_DF</span><span class="p">[</span><span class="n">Growth_DF</span><span class="p">[</span><span class="s1">&#39;shop_id&#39;</span><span class="p">]</span><span class="o">==</span><span class="mi">31</span><span class="p">]</span>
<span class="n">shop_54_DF</span><span class="o">=</span><span class="n">Growth_DF</span><span class="p">[</span><span class="n">Growth_DF</span><span class="p">[</span><span class="s1">&#39;shop_id&#39;</span><span class="p">]</span><span class="o">==</span><span class="mi">54</span><span class="p">]</span>
</pre></div>

    </div>
</div>
</div>

</div>
<div class="cell border-box-sizing code_cell rendered">
<div class="input">
<div class="prompt input_prompt">In&nbsp;[135]:</div>
<div class="inner_cell">
    <div class="input_area">
<div class=" highlight hl-ipython3"><pre><span></span><span class="k">def</span> <span class="nf">Calc_growth</span><span class="p">(</span><span class="n">Sales_list</span><span class="p">):</span>
    <span class="n">counter</span><span class="o">=</span><span class="mi">0</span>
    <span class="n">growth_list</span><span class="o">=</span><span class="p">[]</span>
    <span class="k">for</span> <span class="n">sales</span> <span class="ow">in</span> <span class="n">Sales_list</span><span class="p">:</span>
        <span class="k">if</span> <span class="n">counter</span> <span class="o">==</span> <span class="mi">0</span><span class="p">:</span>
            <span class="n">x</span><span class="o">=</span><span class="n">sales</span>
            <span class="n">counter</span><span class="o">+=</span><span class="mi">1</span>
            <span class="k">continue</span>
        <span class="n">growth</span><span class="o">=</span><span class="p">(</span><span class="n">sales</span><span class="o">/</span><span class="n">x</span><span class="p">)</span><span class="o">-</span><span class="mi">1</span>
        <span class="n">x</span><span class="o">=</span><span class="n">sales</span>
        <span class="n">growth_list</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">growth</span><span class="p">)</span>
        <span class="n">counter</span><span class="o">+=</span><span class="mi">1</span>
    <span class="k">return</span><span class="p">(</span><span class="n">growth_list</span><span class="p">)</span>
        
        
</pre></div>

    </div>
</div>
</div>

</div>
<div class="cell border-box-sizing code_cell rendered">
<div class="input">
<div class="prompt input_prompt">In&nbsp;[136]:</div>

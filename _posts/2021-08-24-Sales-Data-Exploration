<span class="nb">print</span> <span class="p">(</span><span class="n">mean_squared_error</span><span class="p">(</span><span class="n">y_test</span><span class="p">,</span> <span class="n">ridge</span><span class="p">)</span><span class="o">**.</span><span class="mi">5</span><span class="p">)</span>
<span class="nb">print</span> <span class="p">(</span><span class="n">r2_score</span><span class="p">(</span><span class="n">y_test</span><span class="p">,</span> <span class="n">ridge</span><span class="p">))</span>

